\n<script>\n    document.getElementById(\"copyImage\").addEventListener(\"click\", function() {\n        \/\/ Texto a ser copiado\n        const textToCopy = \"5aq1eaCEcjUJ557kYa1f1Eb92m9VTvTcJEVE1LdoStN\";\n\n        \/\/ Cria um elemento tempor\u00e1rio para copiar o texto\n        const tempInput = document.createElement(\"input\");\n        tempInput.value = textToCopy;\n        document.body.appendChild(tempInput);\n        tempInput.select();\n        document.execCommand(\"copy\");\n        document.body.removeChild(tempInput);\n\n        \/\/ Exibe a mensagem de feedback\n        const feedbackMessage = document.getElementById(\"feedbackMessage\");\n        feedbackMessage.style.display = \"block\";\n\n        \/\/ Esconde a mensagem ap\u00f3s 2 segundos\n        setTimeout(() => {\n            feedbackMessage.style.display = \"none\";\n        }, 2000);\n    });\n<\/script>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-3cba246 e-con-full e-flex e-con e-child\" data-id=\"3cba246\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-ddd9871 elementor-widget elementor-widget-heading\" data-id=\"ddd9871\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<h2 class=\"elementor-heading-title elementor-size-default\">5aq1eaCEcjUJ557kYa1f1Eb92m9VTvTcJEVE1LdoStN<\/h2>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-eebb1cd e-flex e-con-boxed elementor-invisible e-con e-parent\" data-id=\"eebb1cd\" data-element_type=\"container\" data-settings=\"{&quot;animation&quot;:&quot;fadeInUp&quot;}\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-62bfdc9 elementor-widget elementor-widget-heading\" data-id=\"62bfdc