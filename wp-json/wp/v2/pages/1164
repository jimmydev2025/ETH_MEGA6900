graus\n      return Math.floor(Math.random() * 31) - 15;\n    }\n\n    function changeImages() {\n      const gallery = document.querySelectorAll(\".gallery-container img\");\n      const usedImages = new Set();\n\n      gallery.forEach(img => {\n        let randomImage;\n        let rotation;\n        do {\n          randomImage = images[Math.floor(Math.random() * images.length)];\n          rotation = getRandomRotation(); \/\/ Obter rota\u00e7\u00e3o aleat\u00f3ria\n        } while (usedImages.has(randomImage));\n        usedImages.add(randomImage);\n        img.src = randomImage;\n        img.style.transform = `rotate(${rotation}deg)`; \/\/ Aplica a rota\u00e7\u00e3o aleat\u00f3ria\n      });\n    }\n  <\/script>\n<\/body>\n<\/html>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-6724e74 e-flex e-con-boxed elementor-invisible e-con e-parent\" data-id=\"6724e74\" data-element_type=\"container\" data-settings=\"{&quot;animation&quot;:&quot;fadeInUp&quot;}\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t<div class=\"elementor-element elementor-element-826b45f e-flex e-con-boxed e-con e-child\" data-id=\"826b45f\" data-element_type=\"container\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-00d854c elementor-invisible elementor-widget elementor-widget-image\" data-id=\"00d854c\" data-element_type=\"widget\" data-settings=\"{&quot;_animation&quot;:&quot;fadeInLeft&quot;}\" data-widget_type=\"image.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" decoding=\"async\" width=\"780\" height=\"881\" src=\"https:\/\/torosol.vip\/wp-content\/uploads\/2025\/01\/Socials-1.webp\" class=\"attachment-large size-large wp-image-1511\" alt=\"\" srcset=\"https:\/\/torosol.vip\/wp-content\/uploads\/2025\/01\/Socials-1.webp 780w, https:\/\/torosol.vip\/wp-content\/uploads\/2025\/0